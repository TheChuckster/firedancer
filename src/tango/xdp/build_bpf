clang -g -S -D__KERNEL__ -D__ASM_SYSREG_H -emit-llvm -O1 -Wno-unused-value -Wno-pointer-sign -Wno-compare-distinct-pointer-types \
        -Wno-gnu-variable-sized-type-not-at-end -Wno-address-of-packed-member -Wno-tautological-compare \
        -Wno-unknown-warning-option -c fd_xdp_bpf_udp.c -o fd_xdp_bpf_udp.ll

llc -march=bpf --filetype=asm -o fd_xdp_bpf_udp.s fd_xdp_bpf_udp.ll
llc -march=bpf --filetype=obj -o fd_xdp_bpf_udp.o fd_xdp_bpf_udp.ll

